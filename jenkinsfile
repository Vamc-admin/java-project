pipeline{
    
    agent any

    stages {

        stage('clean worspace'){

            steps{

                script {
                    cleanWs()
                }
            }
        }
        stage('git checkout'){

            steps{
                git branch: 'master', credentialsId: 'github', url: 'https://github.com/Vamc-admin/java-project.git'
            }
        }
        stage('compile'){
            steps{
                script{
                    sh 'mvn compile'
                }
            }
        }
        stage('Build'){
            steps{
                sh 'mvn clean install -e'
            }
        }
    }
}